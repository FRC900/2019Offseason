<?xml version="1.0"?>
<!-- Simple definition of 2 joints (motors) to test with -->
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="frcrobot">
	<xacro:property name="base_height" value=".15" /> 
	<xacro:property name="base_width" value=".75" /> 
	<xacro:property name="base_depth" value=".75" /> 

	<link name="base_link">
		<visual>
			<geometry>
				<box size="${base_width} ${base_depth} ${base_height}"/>
			</geometry>
			<material name="blue">
				<color rgba="0 0 1 1"/>
			</material>
		</visual>
	</link>
	<link name="link_1">
		<visual>
			<geometry>
				<box size="${base_width} ${base_depth} ${base_height}"/>
			</geometry>
			<material name="blue">
				<color rgba="0 0 1 1"/>
			</material>
		</visual>
	</link>
		<joint name="x_linear_joint" type="continuous">
			<parent link="base_link"/>
			<child link="link_1"/>
			<origin xyz="0 0 0" rpy="0 0 0"/>
		</joint>
	<link name="link_2">
		<visual>
			<geometry>
				<box size="${base_width} ${base_depth} ${base_height}"/>
			</geometry>
			<material name="blue">
				<color rgba="0 0 1 1"/>
			</material>
		</visual>
	</link>
		<joint name="y_linear_joint" type="continuous">
			<parent link="base_link"/>
			<child link="link_2"/>
			<origin xyz="0 0 0" rpy="0 0 0"/>
		</joint>
	<link name="link_3">
		<visual>
			<geometry>
				<box size="${base_width} ${base_depth} ${base_height}"/>
			</geometry>
			<material name="blue">
				<color rgba="0 0 1 1"/>
			</material>
		</visual>
	</link>
		<joint name="z_rotation_joint" type="continuous">
			<parent link="base_link"/>
			<child link="link_3"/>
			<origin xyz="0 0 0" rpy="0 0 0"/>
		</joint>
</robot>
